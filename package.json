{
  "name": "bundlib-starter",
  "version": "0.0.0",
  "description": "My awesome library built using Bundlib",
  "keywords": [],
  "type": "commonjs",
  "exports": {
    "import": "./dist/lib.mjs",
    "default": "./dist/lib.cjs"
  },
  "main": "./dist/lib.cjs",
  "module": "./dist/lib.mjs",
  "jsnext:main": "./dist/lib.mjs",
  "browser": "./dist/lib.umd.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "clean:cache": "rimraf node_modules/.cache/{bundlib,jest}",
    "clean:build": "rimraf dist coverage",
    "clean": "run-p clean:*",
    "lint:test": "eslint __test__/**/*.ts",
    "lint:src": "eslint src/**/*.ts",
    "lint": "run-p lint:*",
    "build": "bundlib",
    "dev": "bundlib -dw",
    "test": "jest --color -w 3",
    "prerelease": "run-s clean lint test build",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.3",
    "bundlib": "^0.18.4",
    "chokidar": "^3.5.2",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/bundlib-starter.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/bundlib-starter/issues"
  },
  "homepage": "https://github.com/manferlo81/bundlib-starter#readme",
  "license": "MIT"
}
